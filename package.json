{
  "name": "url-short",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd front-end && npm run build",
    "build": "concurrently \"npm run build:backend\" \"npm run build:frontend\"",
    "start": "concurrently --kill-others  \"cd backend && npm run start\" \"cd front-end && npm run start\"",
    "start:frontend":"cd front-end && npm run start",
    "start:backend": "cd backend && npm run start",
    "fresh:install": "npm install && concurrently \"cd backend && npm install\" \"cd front-end && npm install\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiaTzatzikiMiaPatates/url-short.git"
  },
